<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Index</servlet-name>
    <servlet-class>com.softserve.academy.demo1.servletes.HomeController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.softserve.academy.demo1.servletes.LoginController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>com.softserve.academy.demo1.servletes.RegisterController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Item</servlet-name>
    <servlet-class>com.softserve.academy.demo1.servletes.ItemController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Item</servlet-name>
    <url-pattern>/item/1</url-pattern>
  </servlet-mapping>
</web-app>
